@using Winkompass_Mobil.Code
@SessionManager.Manager.Responsive.HtmlDoc
<html>
<head>
    <!--jQuery Javascript and jQuery UI Javascript-->
    <script src="http://code.jquery.com/jquery-1.11.1.min.js"></script>
    <script src="http://code.jquery.com/ui/1.11.1/jquery-ui.min.js"></script>

<!--General CSS for the site-->
<link rel="stylesheet" href="~/Content/Site.css">
<!--Material Design Lite CSS-->
<link rel="stylesheet" href="https://code.getmdl.io/1.1.3/material.teal-blue.min.css"/>

<!--Font Awesome CSS-->
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css">
<!--Material Icons CSS-->
<link rel="stylesheet" href="~/Content/bootstrap.min.css">

<!--Material Design Lite Javascript-->
<script defer src="https://code.getmdl.io/1.1.3/material.min.js"></script>




<title>@RenderSection("title", true)</title>
@if (SessionManager.Manager.Responsive is html4)
{
    <style type="text/css">
        /*Remove some default functionality to better have control over the layout*/
        a img { border: 0; }

        div, html, body, form, table, tr, td {
            margin: 0;
            padding: 0;
        }

        /*Standardizing END*/

        body { width: 236px; }

        body * { font-size: 20px; }



        .topnav {
            background-color: #ababab;
            width: 236px;
        }

        body #siteTop { padding: 3px 2px; }



        .siteWrapper {
            margin: 0 11px;
            text-align: center;
        }

        .headerText { font-size: 20px; }


        .button, .menuButton { height: 50px; }

        .element { width: 216px; }

        .elementInputHack {
            height: 35px;
            padding-top: 11px;
            width: 212px;
        }

        .tinyBotSpace { margin-bottom: 5px; }

        .smallBotSpace { margin-bottom: 10px; }

        .mediumBotSpace { margin-bottom: 15px; }

        .largeBotspace { margin-bottom: 20px; }

        .templateChooserBot, .templateChooserTop {
            border: 1px solid black;
            border-collapse: collapse;
        }

        .templateChooserTop { border-bottom: 0; }

        .templateChooserBot { border-top: 0; }

        .templateChooserTop .iconButtonContainer {
            padding: 6px 7px 3px 7px;
            padding-top: 10px;
        }

        .templateChooserTop .iconButtonContainer img { margin-bottom: -3px; }

        .iconButtonContainer {
            border-bottom: 0;
            border-collapse: collapse;
            border-left: 1px solid black;
            border-right: 0;
            border-top: 0;
            margin: 0;
            padding: 2px;
        }

        .templateText {
            padding: 0 2px;
            width: 133px;
        }

        .templateName { /*Border hack to ensure space between buttons*/
            display: block;
        }

        .templateButton {
            background-color: #CCCCCC;
            border: 1px solid black;
            border-collapse: collapse;
            margin: 2px;
            padding: 2px;
            width: 45%;
        }

        .left { text-align: left; }

        .measureHidden {
            left: 0;
            position: absolute;
            top: 0;
            visibility: hidden;
        }

        .templateChoiceFirst, .templateChoice {
            border-bottom: 2px #CCCCCC solid;
            border-collapse: collapse;
            overflow-wrap: break-word;
            overflow-x: auto;
            padding-bottom: 3px;
        }

        .templateChoiceFirst { border-top: 2px #CCCCCC solid; }

        .buttonEmulation {
            background-color: #c9cde9;
            border-bottom: 1px solid #090733;
            border-left: 1px solid white;
            border-right: 1px solid #090733;
            border-top: 1px solid white;
            color: black;
            display: block;
            height: 26px;
            text-decoration: none;
        }

        .buttonEmulationP {
            background-color: black;
            border-collapse: collapse;
            height: 26px;
            padding: 1px 2px 2px 1px;
            width: 213px;
        }

        .lastScans td {
            border: 1px solid black;
            border-spacing: 0;
            font-size: 10px;
            padding: 1px;
        }

        .lastScans tr { border-spacing: 0; }

        .lastScans {
            border-collapse: collapse;
            border-spacing: 0;
        }

        #scanResult span, #result, #result * { font-size: 16px; }

        .scanTableItem { width: 99%; }

        #actionMessage, #actionMessageLabel { font-size: 16px; }

        /*Bootstrap classes to use for color*/

        .text-primary { color: #72bbff; }

        .text-primary b { color: #31708f; }

        .text-success { color: #3c765b; }

        .text-success b { color: #3c763d; }

        .text-danger { color: #a94442; }

        .text-danger b { color: #ba4442; }
    </style>
}
else
{
    <text>@SessionManager.Manager.Responsive.CssLink</text>
}
@SessionManager.Manager.Responsive.ViewPort
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
<meta http-equiv="SIP" content="manual">
@SessionManager.Manager.Responsive.CustomHeaderVariables

</head>
<body>
    
<div class="mdl-layout mdl-js-layout mdl-layout--fixed-header">
    <header class="mdl-layout__header margin-bottom-20">
        <div class="mdl-layout__header-row back-button-padding">

            <!-- Navigation. We hide it in small screens. -->
            <nav class="mdl-navigation">
                <a id="backArrow" class="pull-left" style="padding-right: 5px">
                    <i class="fa fa-arrow-left fa-lg mdl-color-text--white" aria-hidden="true"></i>
                </a>
                <a class="pull-left" href="@Url.Action(MVC.Home.Index())">
                    <i class="fa fa-home fa-lg mdl-color-text--white" aria-hidden="true"></i>
                </a>
            </nav>
            <!-- Add spacer, to align navigation to the right -->
            <div class="mdl-layout-spacer"></div>
            <!-- Title -->
            <h4 class="pull-right">@RenderSection("siteHeader", true)</h4>


        </div>
    </header>
    <main class="mdl-layout__content ">
        <div class="page-content ">
            <div class="siteWrapper" style="margin-bottom: 20px;">
                @RenderBody()
            </div>
        </div>
    </main>
</div>


<script type="text/javascript">
    function goBack() {
        history.back();
    }


</script>

    <script>
        
        var title = document.title.toLowerCase();
        var backbutton = document.getElementById("backArrow");
        var currentURL = window.location.href.toLowerCase();

        if (currentURL.indexOf("list") > -1) {
            backbutton.addEventListener('click',
                function () {
                    window.location = "/Home/Index";
                });
        }

        else if (title.match("lager")){
            backbutton.addEventListener('click',
                function() {
                    window.location = "/Storage/StorageList";
                });

        }

        else if (title.match("indkøb")) {
            backbutton.addEventListener('click',
                function() {
                    window.location = "/Purchase/PurchaseList";
                });
        }

        else if(title.match("ordrer")){
            backbutton.addEventListener('click',
                function() {
                    window.location = "/Order/OrderList";
                });
        }

</script>
@RenderSection("BottomScripts", false)

</body>

</html>